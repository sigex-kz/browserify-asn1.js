{
  "name": "browserify-asn1.js",
  "version": "1.0.0",
  "description": "Browserify ASN1.js (https://github.com/indutny/asn1.js/) library",
  "main": "index.js",
  "scripts": {
    "http-server": "http-server ./",
    "build": "browserify index.js --plugin tinyify --standalone asn1 -o bundle/browserified-asn1.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsenko/browserify-asn1.js.git"
  },
  "keywords": [
    "asn1.js",
    "browserify"
  ],
  "author": "vsenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vsenko/browserify-asn1.js/issues"
  },
  "homepage": "https://github.com/vsenko/browserify-asn1.js#readme",
  "devDependencies": {
    "browserify": "^17.0.0",
    "http-server": "^14.1.0",
    "tinyify": "^3.0.0"
  },
  "dependencies": {
    "@sigex-kz/asn1.js-rfc5652": "^1.0.3",
    "asn1.js": "^5.4.1",
    "asn1.js-rfc5280": "^3.0.0",
    "buffer": "^6.0.0"
  }
}
